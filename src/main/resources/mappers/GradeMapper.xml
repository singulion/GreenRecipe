<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.greenrecipe.main.service.dao.GradeDAO">





	<!-- JG -->
	
	<select id="getGradeInfo" resultType="com.greenrecipe.main.vo.GradeVO" >
		select * from grade where grade_id = #{grade_id}
	</select>

	<select id="getGrade" resultType="com.greenrecipe.main.vo.GradeVO" >
		select * from grade where grade_name = #{grade_name}
	</select>
	



</mapper>